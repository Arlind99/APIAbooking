@model IEnumerable<APIAbooking.Models.Room>

<!Docktype html>
    <head>
        <title>Dashboard for Owner</title>
        <a rel="stylesheet" type="text/css" href="~/lib/bootstrap/dist/css/bootstrap.css"></a>
    </head>
    <body>
        <a asp-action="Create" >Create e new post</a>
        <form class="" method="post">
            <table class="table table-bordered table-striped">
                <thead>
                    <tr>
                        
                        <th>Max guest</th>
                        <td>Number of bathroom</td>
                        <th>Contry</th>
                        <th>Street</th>
                        <th>City</th>
                        <th>Number of House</th>
                        <th>Zip code</th>
                        <th>What amentities do you offer</th>
                        <th>A sepecial item</th>
                        <th>Description</th>
                        <th>Phone number</th>
                        <th>Checkin</th>
                        <th>Checkout</th>
                        <th>Price</th>
                        <th>Is userd</th>
                    </tr>
                    
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                    <tr>
                        
                        <td>@item.MaxGuest</td>
                        <td>@item.NrBathroom</td>
                        <td>@item.Country</td>
                        <td>@item.Street</td>
                        <td>@item.City</td>
                        <td>@item.NoHouse</td>
                        <td>@item.Zip</td>
                        <td>@item.WhatSpaceCanUseGuests</td>
                        <td>@item.SpecialItem</td>
                        <td>@item.Describe</td>
                        <td>@item.PhoneNumber</td>
                        <td>@item.Checkin</td>
                        <td>@item.Checkout</td>
                        <td>@item.Price</td>
                        <td>@item.Reserved</td>
                        <a asp-action="Edit" asp-controller="Room" asp-route-id="@item.RoomId" style="color:green">Edit</a>
                        <a asp-action="Delete" asp-controller="Room" asp-route-id="@item.RoomId" style="color:red">Delete</a>
                    </tr>

                    }
                </tbody>
            </table>
        </form>
        <a asp-action="OwnerID" asp-route-id="OwnerId">Personal info</a>
    </body>
    </!Docktypehtml>
